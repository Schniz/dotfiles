{
  "graphql.filetypes": ["graphql"],
  "codeLens.enable": true,
  "prettier.eslintIntegration": true,
  "prisma.prismaFmtBinPath": "",
  "coc.preferences.formatOnSaveFiletypes": [
    "elixir",
    "prisma",
    "rust",
    "markdown",
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact",
    "javascript.jsx",
    "json",
    "jsonc",
    "ruby",
    "eruby",
    "prisma"
  ],
  "solargraph.bundlerPath": "~/.rbenv/shims/bundle",
  "solargraph.useBundler": true,
  "coc.source.emoji.filetypes": [
    "markdown",
    "markdown.mdx",
    "markdown.ghpull",
    "eruby"
  ],
  "javascript.suggestionActions.enabled": false,
  "tailwindCSS.emmetCompletions": true,
  "tailwindCSS.trace.server": "verbose",
  "tailwindCSS.jsLanguages": [
    "javascript",
    "javascriptreact",
    "reason",
    "typescriptreact",
    "typescript.jsx",
    "javascript.jsx"
  ],
  "rust-analyzer.procMacro.enable": true
}
